{
  "name": "kd-monitor",
  "version": "0.3.0",
  "description": "前端监控sdk",
  "main": "./dist/index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.umd.js"
  },
  "browser": "./dist/index.umd.js",
  "scripts": {
    "bootstrap": "pnpm install",
    "commit": "czg",
    "release": "release-it --config .release-it.cjs",
    "build": "vite build",
    "publish": "pnpm publish ./dist --no-git-checks",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "lint": "pnpm format && pnpm lint:eslint",
    "format": "prettier --write .",
    "lint:eslint": "eslint --fix --ext .vue,.ts,.tsx,.jsx,.js,.json,.md --max-warnings 0 --cache",
    "lint:style": "stylelint \"**/*.{css,scss,sass,vue}\" --allow-empty-input --fix",
    "clean": "rimraf node_modules **/*/node_modules",
    "build:pre": "rimraf dist"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@release-it/conventional-changelog": "^7.0.0",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "release-it": "^16.1.3",
    "rimraf": "^5.0.1",
    "rollup": "^4.1.4",
    "typescript": "^5.2.2",
    "vite": "5.2.6",
    "vue": "^3.4.30"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@9.6.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
